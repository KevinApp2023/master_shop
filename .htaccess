
RewriteEngine On
RewriteRule ^$ /src/home.php [L,QSA]

# RewriteEngine On
 RewriteRule ^p/([^/]+)/([^/]+)$ /src/home.php?category=$1&_=$2 [L,QSA]

 RewriteRule ^([^/]+)$ /src/home.php?category=&_=$1 [L,QSA]





RewriteRule ^ofertas$ /src/ofertas.php [L,QSA]

# Products
RewriteRule ^product/([^/]+)/([^/]+)$ /src/product.php?id=$1&_=$2 [L,QSA]



#Cart
RewriteRule ^cart/add$ /config/agregar_cart.php [L,QSA]
RewriteRule ^cart/products$ /config/cart.php [L,QSA]
RewriteRule ^cart/eliminar_data_cart$ /config/eliminar_data_cart.php [L,QSA]
